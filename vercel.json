{
    "version":2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ],
    "functions": {
      "api/**/*.{js,ts}": {
        "maxDuration": 150,
        "memory": 1024,
        "limits": {
          "body": 10000000
        }
      }
    }
  }